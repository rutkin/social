        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Generate Random Names">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.Random;

String[] firstNames = ["Иван", "Петр", "Алексей", "Сергей", "Дмитрий", "Андрей", "Максим", "Николай", "Владимир", "Александр"];
String[] lastNames = ["Иванов", "Петров", "Смирнов", "Кузнецов", "Попов", "Васильев", "Соколов", "Михайлов", "Новиков", "Федоров"];

Random random = new Random();
String firstName = firstNames[random.nextInt(firstNames.length)];
String lastName = lastNames[random.nextInt(lastNames.length)];

// Take first 4 letters of each name, or the whole name if it's shorter
vars.put("FIRST_NAME", firstName.length() > 4 ? firstName.substring(0, 4) : firstName);
vars.put("LAST_NAME", lastName.length() > 4 ? lastName.substring(0, 4) : lastName);</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223PreProcessor>
